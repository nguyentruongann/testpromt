	"font_size": 14,
	"html": "\n<head>\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n\n    <style>\n        .print-format div {\n            font-size: 6pt;\n            page-break-after: auto;\n            page-break-inside: avoid;\n        }\n    </style>\n</head>\n\n    {% set doc = frappe.get_doc('Item', doc.name) %}\n<div style=\"justify-content: space-between;min-width: {{ doc.custom_page_type or 110 }}mm;max-width: {{ doc.custom_page_type  or 110}}mm;display:flex;flex-wrap: wrap;\">\n    {% if doc.barcodes %}\n        {% set price = frappe.get_all('Item Price', filters={'item_code': doc.name, 'selling': 1}, fields=['currency', 'price_list_rate'], limit=1, order_by='creation desc') %}\n        {% for d in range(0, doc.custom_barcode_qty or 1) %}\n        <div style=\"min-width: 35mm;max-width: 35mm;\">\n            <div><center>{{ doc.item_name }}</center></div>\n            <div><center>{{ doc.name}}</div></center>\n            <div><center><img style=\"width: 100%;\" src={{ doc.custom_barcode_data }}></center></div>\n            <div><center>{{ doc.barcodes[0].barcode }}</div></center>\n            <div style=\"margin-bottom: 5px;\"><b><center>{{ frappe.utils.fmt_money(price[0].price_list_rate,precision = 0, format='#.###') if price else '' }} {{ price[0].currency if price else '' }}</b></center></div>\n        </div>\n        {% endfor %}\n    {% else %}\n    <center><h1>Ch\u01b0a c\u00f3 m\u00e3 barcode</h1></center>\n    {% endif %}\n</div>\n\n",
